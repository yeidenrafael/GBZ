@model System.Data.DataTable
@{
    ViewBag.ViewTitle = "";
}
@Html.Partial("_SearchFields")

@{
    if (ViewData["viewName"] != null)
    {
        Html.RenderPartial("_HeaderGridView");
        Html.RenderPartial("_GridViewSearch", Model, new ViewDataDictionary { { "viewName", ViewData["viewName"] }, { "value", ViewData["value"] }, { "crop", ViewData["crop"] }, { "moduleId", ViewData["moduleId"] } });
    }
}