  <sys_dataview_sql>
    <dataview_name>gbz_get_box</dataview_name>
    <database_engine_tag>sqlserver</database_engine_tag>
    <sql_statement>SELECT
  i.inventory_id,
  i.storage_location_part1,
  i.storage_location_part2,
  i.storage_location_part3,
  i.storage_location_part4,
  i.owned_by,
  '' + RTRIM(i.inventory_number_part1) + ' ' + RTRIM(CAST(i.inventory_number_part2 AS nchar(6))) + ' ' + COALESCE(inventory_number_part3, '') AS inventory_number
FROM inventory i
WHERE 
(RTRIM(i.inventory_number_part1) + ' ' + RTRIM(CAST(i.inventory_number_part2 AS nchar(6))) + ' ' + COALESCE(inventory_number_part3, '') IN (:inventorynumber))
OR (i.storage_location_part1 = :car
AND i.storage_location_part2 = :sec
AND i.storage_location_part3 = :box)
ORDER BY i.storage_location_part1,
i.storage_location_part2,
i.storage_location_part3,
convert(int, i.storage_location_part4)
</sql_statement>
  </sys_dataview_sql>
