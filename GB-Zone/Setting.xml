<?xml version="1.0" encoding="utf-8" ?>
<Settings>
  <crop id="01" name="Wheat" url="http://localhost/gringlobal/gui.asmx">
    <modules id="Inventory" title="Inventory" description="You can search information about Inventory items by Storage location, Inventory number or INTRID" icon="pe-7s-note2" action="Index" controller="Search">
      <form layout="vertical" cssClass="form-group">
        <field id="01" type="text" placeholder="CAR-SEC-BOX" visible="true">
          <actions>
            <parameters>
              <suppressExceptions>false</suppressExceptions>
              <dataviewName>gbz_get_inventory</dataviewName>
              <delimitedParameterList>:car={0};:sec={0};:box={0};:inventory_number1=;:inventory_number2=;:inventory_number3=;:intrid=;</delimitedParameterList>
              <offset>0</offset>
              <limit>0</limit>
              <options>1</options>
              <separator>45</separator>
            </parameters>
            <columns>
              <column link="true" colreference="inventory_number_part1" viewreference="03" >accession_number</column>
              <column link="true" colreference="inventory_number" viewreference="02" >inventory_number</column>
            </columns>
          </actions>
        </field>

        <field id="02" type="text"  placeholder="Inventory Number" visible="true">
          <actions>
            <parameters>
              <suppressExceptions>false</suppressExceptions>
              <dataviewName>gbz_get_inventory</dataviewName>
              <delimitedParameterList>:car=;:sec=;:box=;:inventory_number1={0};:inventory_number2={0};:inventory_number3={0};:intrid=;</delimitedParameterList>
              <offset>0</offset>
              <limit>0</limit>
              <options>1</options>
              <separator>32</separator>
            </parameters>
            <columns>
              <column link="true" colreference="inventory_number_part1" viewreference="03" >accession_number</column>
              <column link="true" colreference="storage_location" viewreference="01" >storage_location</column>
            </columns>
          </actions>
        </field>

        <field id="03" type="text" placeholder="INTRID" visible="true">
          <actions>
            <parameters>
              <suppressExceptions>false</suppressExceptions>
              <dataviewName>gbz_get_inventory</dataviewName>
              <delimitedParameterList>:car=;:sec=;:box=;:inventory_number1=;:inventory_number2=;:inventory_number3=;:intrid={0};</delimitedParameterList>
              <offset>0</offset>
              <limit>0</limit>
              <options>1</options>
              <separator>127</separator>
            </parameters>
            <columns>
              <column header="true">accession_number</column>
              <column header="true">gid</column>
              <column header="true">plant_name</column>
              <column header="true">taxonomy</column>
              <column header="true">country</column>
              <column link="true" colreference="inventory_number" viewreference="02" >inventory_number</column>
              <column link="true" colreference="storage_location" viewreference="01" >storage_location</column>
            </columns>
          </actions>
        </field>
      </form>
    </modules>

    <modules id="Orders" title="Orders" description="You can search information about Seed Orders Request" icon="pe-7s-box1" action="Index" controller="Search">
      <form layout="vertical" cssClass="form-group">
        <field id="04" type="number" placeholder="Order Id" visible="true">
          <actions>
            <parameters>
              <suppressExceptions>false</suppressExceptions>
              <dataviewName>gbz_get_order_request_item</dataviewName>
              <delimitedParameterList>:orderrequestid={0};</delimitedParameterList>
              <offset>0</offset>
              <limit>0</limit>
              <options>1</options>
              <separator>127</separator>
            </parameters>
            <columns>
              <column header="true">order_request_id</column>
              <column header="true">local_number</column>
              <column header="true">web_order_request_id</column>
              <column header="true">order_type</column>
              <column header="true">cooperator_name</column>
              <column header="true">organization</column>
              <column header="true">final_country</column>
              <column link="true" colreference="inventory_number_part1" viewreference="03" >accession_number</column>
            </columns>
          </actions>
        </field>

        <field id="03" type="text" placeholder="INTRID" visible="false">
          <actions>
            <parameters>
              <suppressExceptions>false</suppressExceptions>
              <dataviewName>gbz_get_inventory</dataviewName>
              <delimitedParameterList>:car=;:sec=;:box=;:inventory_number1=;:inventory_number2=;:inventory_number3=;:intrid={0};</delimitedParameterList>
              <offset>0</offset>
              <limit>0</limit>
              <options>1</options>
              <separator>127</separator>
            </parameters>
            <columns>
              <column header="true">accession_number</column>
              <column header="true">gid</column>
              <column header="true">plant_name</column>
              <column header="true">taxonomy</column>
              <column header="true">country</column>
            </columns>
          </actions>
        </field>
      </form>
    </modules>
  </crop>

